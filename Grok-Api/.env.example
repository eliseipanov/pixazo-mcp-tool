# Grok-Api Environment Variables

# SOCKS5 Proxy Configuration
# Set to 'true' to enable SOCKS5 proxy for all requests
USE_SOCKS=true

# SOCKS5 proxy URL (required if USE_SOCKS=true)
# Format: socks5://user:password@ip:port
SOCKS=socks5://127.0.0.1:16379

# Debug Mode
# Set to 'true' to enable debug logging
DEBUG=false

# API Server Configuration
# Host and port for the API server
HOST=0.0.0.0
PORT=6969

# Workers
# Number of worker processes for the API server
WORKERS=5
